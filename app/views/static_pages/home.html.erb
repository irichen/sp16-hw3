<h1>Users</h1>
<% @users.each do |user| %>
    <%= user.username %> <%= user.email %> <%= user.age %>
<% end %>


<h1>Cats</h1>
<% @cats.each do |cat| %>
    <%= cat.name %> says <%= cat.meow %>
<% end %>

<h1>Todos</h1>
<% @todos.each do |todo| %>
    <%= todo.tasks %>
    <% if todo.finished %>
        [finished]
    <% else %>
        [not finished]
    <% end %>
<% end %>

